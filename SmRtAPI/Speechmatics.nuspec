<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Speechmatics</id>
		<version>2.0.0</version>
		<authors>Speechmatics</authors>
		<owners>Speechmatics</owners>
		<readme>docs/README.md</readme>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/speechmatics/speechmatics-dotnet</projectUrl>
		<description>
			.NET client for the Speechmatics real-time speech-recognition API.
			Async interface for the WebSocket API either for self-hosted instances or speechmatics.com
			To start using, simply create an account and an API key on https://portal.speechmatics.com and follow the Readme.
		</description>
		<copyright>Speechmatics</copyright>
		<tags>asr,speech-to-text,speech-recognition,real-time,speechmatics</tags>
