<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Speechmatics</id>
		<version>2.0.0</version>
		<authors>Speechmatics</authors>
		<readme>docs/README.md</readme>
		<repository type="git" url="https://github.com/speechmatics/speechmatics-dotnet.git"/>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/speechmatics/speechmatics-dotnet</projectUrl>
		<description>
			.NET client for the Speechmatics real-time speech-to-text API.
			Async interface for the websocket API for self-hosted instances or speechmatics.com.
			To begin, create an account and an API key on https://portal.speechmatics.com and follow the README.
		</description>
		<copyright>Speechmatics</copyright>
		<tags>asr,speech-to-text,speech-recognition,real-time,speechmatics</tags>
		<dependencies>
			<group targetFramework=".NETStandard2.1">
				<dependency id="Newtonsoft.Json" version="13.0.3" exclude="Build,Analyzers" />
				<dependency id="System.Net.WebSockets.Client" version="4.3.2" exclude="Build,Analyzers" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="SmRtAPI/bin/Release/netstandard2.1/Speechmatics.dll" target="lib/netstandard2.1/Speechmatics.dll" />
		<file src="SmRtAPI/bin/Release/netstandard2.1/Speechmatics.xml" target="lib/netstandard2.1/Speechmatics.xml" />
		<file src="SmRtAPI/docs/README.md" target="docs/README.md" />
	</files>
</package>
